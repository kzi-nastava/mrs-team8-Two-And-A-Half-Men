<?xml version="1.0" encoding="utf-8"?>
<!-- bg_card_editing.xml -->
<!-- Highlighted card when in edit mode - brighter green border like the web editing state -->
<shape xmlns:android="http://schemas.android.com/apk/res/android"
    android:shape="rectangle">
    <solid android:color="#1E2A1E" />
    <stroke
        android:width="2dp"
        android:color="#C8A800" />
    <corners android:radius="8dp" />
</shape>
