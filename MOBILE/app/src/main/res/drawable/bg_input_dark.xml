<?xml version="1.0" encoding="utf-8"?>
<!-- bg_input_dark.xml -->
<!-- Very dark input background like the web's black input field in edit mode -->
<shape xmlns:android="http://schemas.android.com/apk/res/android"
    android:shape="rectangle">
    <solid android:color="#0D1A0D" />
    <stroke android:width="1dp" android:color="#3D6B3D" />
    <corners android:radius="6dp" />
</shape>
